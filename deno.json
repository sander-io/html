    "embed:example": "deno run -A embed_example.ts"
  },
  "authors": [
    "Sander Hahn <sanderhahn@gmail.com>"
  ],
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.10"
  }
}
